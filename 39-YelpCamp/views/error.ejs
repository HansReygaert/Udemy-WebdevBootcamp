<% layout('/layouts/boilerplate') %> 

<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger mb-3" role="alert">
            <h4 class="alert-heading">Error <%= err.statusCode %> </h4>
            <p class="mt-3 offset-1"><%= err.message %> </p>
            <br>
            <p><%= err.stack %> </p>
          </div>
    </div>
</div>
